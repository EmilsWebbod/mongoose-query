{
  "name": "@ewb/mongoose-query",
  "version": "1.0.3",
  "type": "module",
  "description": "Parse queries and params to mongoose search. Strips dangerous keys and set corrects options with help of query values.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "yarn build:watch",
    "build": "tsc",
    "build:watch": "tsc --watch --incremental",
    "lint": "tslint --project \"./tsconfig.json\"",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "postversion": "git push && git push --tags",
    "prepublish": "yarn test && yarn build",
    "preversion": "yarn build"
  },
  "dependencies": {
    "express-mongo-sanitize": "^2.2.0",
    "http-status": "^1.6.2",
    "jsondiffpatch": "^0.4.1"
  },
  "devDependencies": {
    "@vitest/ui": "^0.31.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mongoose": "^7.2.0",
    "prettier": "^2.8.8",
    "tslint": "^6.1.3",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "peerDependencies": {
    "mongoose": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilswebbod/mongoose-query.git"
  },
  "keywords": [
    "typescript",
    "npm",
    "translate"
  ],
  "author": "Emils Webbod",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emilswebbod/mongoose-query/issues"
  },
  "homepage": "https://github.com/emilswebbod/mongoose-query#readme",
  "files": [
    "lib"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "resolutions": {}
}
